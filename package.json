{
  "name": "@mod-cli/mod-cli",
  "version": "0.2.3",
  "description": "",
  "author": "Jon Roby",
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonroby/mod-cli.git"
  },
  "bugs": {
    "url": "https://github.com/jonroby/mod-cli/issues"
  },
  "homepage": "https://github.com/jonroby/mod-cli#readme",
  "engines": {
    "node": ">=8.0.0 <11.0.0"
  },
  "bin": {
    "mod": "./src/index.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && npm run eslint && npm run prettier",
      "pre-push": "npm run test && npm run eslint && npm run prettier"
    }
  },
  "scripts": {
    "test": "jest",
    "eslint": "./node_modules/.bin/eslint \"./src/**/*.js\"",
    "prettier": "prettier --write \"./src/**/*.js\"",
    "build": "./node_modules/.bin/babel src --out-dir dist/src --ignore \"src/**/__mocks__/*.js\",\"src/**/__tests__/*.js\" && cp package.json LICENSE dist",
    "prepublish": "npm run test && npm run eslint && npm run prettier && npm run build"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-jest": "^22.0.1",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "prettier": "1.15.2"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "shelljs": "^0.8.3"
  }
}
